{
  "name": "@mikecodeur/react-course-app",
  "version": "0.1.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "An abstraction for all my React courses",
  "main": "dist/@mikecodeur/react-course-app.esm.js",
  "types": "dist/@mikecodeur/react-course-app.esm.d.ts",
  "keywords": [],
  "author": "Mike Codeur <contact@mikecodeur.com> (https://www.mikecodeur.com)",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikecodeur/react-course-app"
  },
  "bugs": {
    "url": "https://github.com/mikecodeur/react-course-app/issues"
  },
  "homepage": "https://github.com/mikecodeur/react-course-app#readme",
  "files": [
    "dist",
    "codegen.d.ts",
    "codegen.js",
    "setup-tests.d.ts",
    "setup-tests.js",
    "server.d.ts",
    "server.js",
    "test-utils.d.ts",
    "test-utils.js"
  ],
  "scripts": {
    "start": "npm run dev",
    "predev": "npm run build",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:build": "concurrently \"npm:dev:build:*\"",
    "dev:build:main": "mcr-scripts build --watch --no-clean",
    "dev:build:bundle:main": "mcr-scripts build --bundle esm --no-clean --watch",
    "dev:examples": "cd ./example/react-fundamentals && npm start",
    "setup": "npm run install:main && npm run install:examples && npm run validate && npm run validate:examples",
    "install:main": "npm install",
    "install:examples": "cd ./example/react-fundamentals && npm install",
    "validate:examples": "cd ./example/react-fundamentals && npm run validate",
    "build": "npm run build:main && concurrently \"npm:build:bundle:*\"",
    "build:main": "mcr-scripts build",
    "build:bundle:main": "mcr-scripts build --bundle esm --no-clean",
    "lint": "mcr-scripts lint",
    "test": "mcr-scripts test",
    "test:coverage": "mcr-scripts test --coverage",
    "typecheck": "mcr-scripts typecheck",
    "test:debug": "node --inspect-brk node_modules/mcr-scripts/dist/scripts/test.js --runInBand --no-cache",
    "validate": "npm run build && mcr-scripts validate test:coverage,typecheck,lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/core": "^10.0.35",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reach/tabs": "^0.14.0",
    "@reach/tooltip": "^0.14.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "chalk": "^4.1.0",
    "codegen.macro": "^4.1.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "emotion-theming": "^10.0.27",
    "eslint-plugin-testing-library": "^4.9.0",
    "facepaint": "^1.2.1",
    "glob": "^7.1.6",
    "history": "^4.10.1",
    "is-ci": "^3.0.0",
    "mdx-loader": "^3.0.2",
    "mq-polyfill": "^1.1.8",
    "msw": "^0.28.1",
    "node-match-path": "^0.6.2",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.24.1",
    "raw-loader": "^4.0.2",
    "react-canvas-confetti": "^1.2.1",
    "react-error-boundary": "^3.1.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "replace-in-file": "^6.2.0"
  },
  "devDependencies": {
    "@types/facepaint": "^1.2.1",
    "@types/glob": "^7.1.3",
    "@types/history": "^4.7.8",
    "@types/preval.macro": "^3.0.0",
    "@types/prismjs": "^1.16.6",
    "@types/react-router-dom": "^5.1.7",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "concurrently": "^6.0.0",
    "cssnano": "^4.1.10",
    "eslint-config-kentcdodds": "^19.1.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "identity-obj-proxy": "^3.0.0",
    "mcr-scripts": "0.0.2-aplha",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "preval.macro": "^5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "resolve": "^1.20.0",
    "typescript": "^4.2.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "browserslist": [
    "chrome >= 79",
    "last 2 firefox versions",
    "last 2 edge versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "mcr-scripts pre-commit"
    }
  },
  "eslintConfig": {
    "extends": "./node_modules/mcr-scripts/eslint.js",
    "settings": {
      "jest": {
        "version": 26
      }
    },
    "rules": {
      "no-console": "off",
      "max-lines-per-function": "off",
      "guard-for-in": "off",
      "react/display-name": "off",
      "react/prop-types": "off",
      "import/no-unresolved": "off",
      "@typescript-eslint/sort-type-union-intersection-members": "off",
      "jsx-a11y/accessible-emoji": "off"
    }
  }
}
